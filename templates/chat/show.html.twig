{% extends 'base.html.twig' %}

{% block title %}Chat{% endblock %}

{% block body %}
    <h1>Chat</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ chat.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ chat.nom }}</td>
            </tr>
            <tr>
                <th>Age</th>
                <td>{{ chat.age }}</td>
            </tr>
            <tr>
                <th>Race</th>
                <td>{{ chat.race }}</td>
            </tr>
            <tr>
                <th>Sexe</th>
                <td>{{ chat.sexe }}</td>
            </tr>
            <tr>
                <th>Etat_sante</th>
                <td>{{ chat.etatSante }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ chat.statut }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_chat_index') }}">back to list</a>

    <a href="{{ path('app_chat_edit', {'id': chat.id}) }}">edit</a>

    {{ include('chat/_delete_form.html.twig') }}
